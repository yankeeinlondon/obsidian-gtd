{
	"compilerOptions": {
		"baseUrl": ".",
		"inlineSourceMap": true,
		"inlineSources": true,
		"module": "ESNext",
		"target": "ESNext",
		"allowJs": true,
		"noImplicitAny": true,
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"importHelpers": true,
		"isolatedModules": true,
		"strictNullChecks": true,
		"types": ["obsidian"],
		"typeRoots": ["node_modules/@types", "node_modules/obsidian"],
		"paths": {
			"~/*": ["src/*"]
		},
		"lib": ["DOM", "ES5", "ES6", "ES7", "ES2022"]
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist", "*.mjs"]
}
